analyzeCommits:
  preset: eslint
  releaseRules:
  - type: Docs
    release: minor
prepare:
- "@semantic-release/changelog"
- "@semantic-release/npm"
- path: "@semantic-release/git"
  assets:
  - package.json
  - package-lock.json
  message: |-
    Chore: Release ${nextRelease.version} [skip ci]

    ${nextRelease.notes}
